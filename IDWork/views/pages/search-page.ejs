<!-- Titlebar
================================================== -->
<div id="titlebar">
  <div class="container">
    <div class="ten columns">
      <h2>Results for <%= category %> in <%= place%></h2>
    </div>
  </div>
</div>

<!-- Content
================================================== -->
<div class="jobs container">
  <!-- Widgets -->
  <div class="eight columns">
    <h2>Austin, TX</h2>
    <h4>Stats</h4>
    <div style="display: inline-block">
      <ul>
        <li><img class="stats-img" src="images/users.svg"/> <%= city.population%> <br> <span>Population</span> </li>
        <li><img class="stats-img" src="images/users.svg"/> Average annual salary: <%= city.average_salary_USN%></li>
        <li><img class="stats-img" src="images/sun.svg"/> Average high/low temps: <%= city.temperature%></li>
      </ul>
    </div>
    <div style="display: inline-block">
      <ul>
        <li><img class="stats-img" src="images/calendar.svg"/> Median Age: <%= city.median_age%></li>
        <li><img class="stats-img" src="images/house.svg"/> Median Home Price: <%= city.median_home_price%></li>
        <li><img class="stats-img" src="images/drop.svg"/> Average annual rainfall: <%= city.average_annual_rainfall%></li>
      </ul>
    </div>
    <div style="display: inline-block">
      <ul>
        <li><img class="stats-img" src="images/bar-chart.svg"/> Unemployment rate: <%= city.unemployment_rate%></li>
        <li><img class="stats-img" src="images/hotel.svg"/> Median Monthly rent: <%= city.median_monthly_rate%></li>
        <li><img class="stats-img" src="images/stopwatch.svg"/> Average Commute Time: <%= city.average_commute_time%></li>
      </ul>
    </div>
    <h3>What's like?</h3>
    <p align="justify"><%= city.whats_like%></p>
    <h3>What's the cost of living?</h3>
    <p align="justify"><%= city.cost_of_living%></p>
    <h3>What's the weather like?</h3>
    <p align="justify"><%= city.weather%></p>
    <h3>What's the best way to get around?</h3>
    <p align="justify"><%= city.commuting%></p>
    <h3>Who lives here?</h3>
    <p align="justify"><%= city.who_lives%></p>
    <h3>What is there to do?</h3>
    <p align="justify"><%= city.what_to_do%></p>
  </div>
  <!-- Widgets / End -->

  <!-- Recent Jobs -->
  <h2>Jobs</h2>
  <div class="eight columns jobs">
    <div>
      <ul class="job-list full">
        <% jobs.forEach((job) => {%>

        <li>
          <a href="job-page.html">
            <img src="images/job-list-logo-01.png" alt="" />
            <div class="job-list-content">
              <h4><%=job.title%></h4>
              <div class="job-icons">
                <span
                  ><i class="fa fa-briefcase"></i> <%=job.company_name%></span
                >
                <% if(job.city){%>
                <span><i class="fa fa-map-marker"></i> <%=job.city%></span>
                <%}%>
              </div>
              <!--
			  <p><%=job.description%></p>
			  -->
            </div>
          </a>
          <div class="clearfix"></div>
        </li>
        <% });%>
      </ul>
      <div class="clearfix"></div>

      <!--
		<div class="pagination-container">
			<nav class="pagination">
				<ul>
					<li><a href="#" class="current-page">1</a></li>
					<li><a href="#">2</a></li>
					<li><a href="#">3</a></li>
					<li class="blank">...</li>
					<li><a href="#">22</a></li>
				</ul>
			</nav>

			<nav class="pagination-next-prev">
				<ul>
					<li><a href="#" class="prev">Previous</a></li>
					<li><a href="#" class="next">Next</a></li>
				</ul>
			</nav>
        </div>
        -->
    </div>
  </div>
</div>
