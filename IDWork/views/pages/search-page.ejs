<div id="titlebar">
	<div class="container">
		<div class="ten columns">
			<span>We found 1,412 jobs matching:</span>
			<h2>Web, Software & IT</h2>
		</div>

		<div class="six columns">
			<a href="add-job.html" class="button">Post a Job, Itâ€™s Free!</a>
		</div>

	</div>
</div>


<!-- Content
================================================== -->
<div class="container">
	<!-- Recent Jobs -->
	<div class="eleven columns">
	<div class="padding-right">
		
		<form action="#" method="get" class="list-search">
			<button><i class="fa fa-search"></i></button>
			<input type="text" placeholder="job title, keywords or company name" value=""/>
			<div class="clearfix"></div>
		</form>

		<ul class="job-list full">

            <% jobs.forEach((job) => {%>

			<li><a href="job-page.html">
				<img src="images/job-list-logo-01.png" alt="">
				<div class="job-list-content">
					<h4> <%=job.title%></h4>
					<div class="job-icons">
						<span><i class="fa fa-briefcase"></i> <%=job.company_name%></span>
						<% if(job.city){%>
						<span><i class="fa fa-map-marker"></i> <%=job.city%></span>
						<%}%>
					</div>
					<p><%=job.description%></p>
				</div>
				</a>
				<div class="clearfix"></div>
            </li>
            <% });%>

		</ul>
		<div class="clearfix"></div>

